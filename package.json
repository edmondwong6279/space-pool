{
  "name": "universlabs-stars-vis",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^16.0.0",
    "yarn": "^1.0.0"
  },
  "scripts": {
    "lint": "concurrently yarn:lint:scripts yarn:lint:styles",
    "lint:scripts": "eslint ./src --ext .ts,.tsx",
    "lint:scripts:fix": "eslint ./src --fix --ext .ts,.tsx",
    "lint:styles": "stylelint \"./src/**/*.scss\"",
    "lint:styles:fix": "stylelint \"./src/**/*.scss\" --fix",
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "ci": "concurrently yarn:lint yarn:type-check",
    "fix": "concurrently yarn:lint:scripts:fix yarn:lint:styles:fix"
  },
  "dependencies": {
    "cannon-es": "^0.19.0",
    "dotenv": "^16.0.0",
    "next": "^12.1.6",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "sass": "^1.32.7",
    "three": "^0.141.0",
    "three-orbitcontrols-ts": "^0.1.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.14",
    "@types/three": "^0.141.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.4.12",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.2.1",
    "stylelint": "^14.6.1",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.1.3"
  }
}
